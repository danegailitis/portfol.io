{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "rewrites": [
    {
        "source": "/login",
        "destination": "app"
    },
    {
      "source": "/profile",
      "destination": "app"
    },
    {
      "source": "/",
      "destination": "app"
    }
  ]
}
